<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper
	namespace="site.beyondchasm.teambasketball.common.dao.RegionDao">


	<select id="getRegionList"
		resultType="site.beyondchasm.teambasketball.common.domain.RegionDTO">
		SELECT
		region_code,
		region_name
		FROM
		tbl_region_bais
		WHERE
		USE_YN = 'Y'
	</select>

</mapper>
